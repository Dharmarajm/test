page-history {
	.images_header{
		width: 20%;
		display: block;
	    margin-left: auto;
	    margin-right: auto;
	}
	.bg{
		background-color:#002b3d;
	}
	.icon_header{
		color: white;
        font-size: 25px;
	}
	.btn_detalis{
		background-color: #002b3d;
		color: white;
		border-radius: 5px;
	}
	.tabs{
	    display: block;
	    margin-left: auto;
	    margin-right: auto;
	    text-align: center;
	}	
	.btn_detalis:focus, .btn_detalis:hover, .btn_detalis:active,.btn_detalis.activated
	{
	  color: #0c2a41;
	  background-color: #fcc52a;
	  border-radius: 5px;
	}
	.ionc_color{
		color:#0c2a41;
	}
	.sub_name{
		color:grey;
	}
	.name{
		font-weight:bold;
		color:black;
	}
	.searchbar-md .searchbar-input{
		width:100%;
		box-shadow: 0px 0px;
	}
	.btn_tab{
		color:#0c2a41;
		font-style:bold;
		border-radius:0px;
		box-shadow: 0px 0px;
	}
	.btn_tab:focus, .btn_tab:hover, .btn_tab:active,.btn_tab.activated
	{
	  color: #0c2a41;
	  border-bottom: 2px solid #dd892b !important;
	  border-radius:0px;
	}
}
