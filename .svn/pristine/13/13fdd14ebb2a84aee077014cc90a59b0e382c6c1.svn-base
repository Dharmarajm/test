page-view {
	.toolbar-content {
	    text-align: center;
	    display: flex;
	    align-items: flex-end;
	    justify-content:space-between;
	}
	.header button{
		color:#fff;
	}
	.header img{
	    width: 115px;
	    padding: 5px 0;
	}
	.toolbar-content span:last-child .button[icon-only] {
	    padding: 0;
	    min-width: .9em;
	    width: 25px;
	    height: 25px;
	}
	.toolbar-content span:last-child{
	    padding-right:10px;
	}
	.item-ios ion-avatar ion-img, .item-ios ion-avatar img, .item-md ion-avatar ion-img, .item-md ion-avatar img {
	    object-fit: cover;
	    height:45px;
	    width:45px;
	}
	.item-block .item-inner{
	    border-bottom: 0px !important;
	}
	.list-ios .item-block, .list-ios .item-wrapper .item-block, .list-md .item-block, .list-md .item-wrapper .item-block {
	    border-bottom: 1px solid #d2d2d2;
	}
	.list-ios .item-block:last-child, .list-ios .item-wrapper:last-child .item-block, .list-md .item-block:last-child, .list-md .item-wrapper:last-child .item-block {
	    border-bottom: 0px;
	}
	.user_list ion-label{
		position:relative;
	}
	.user_list ion-label button{
	    position: absolute;
	    right: 0;
	    top: 15%;
	    color:#002b3d;
	}
	.user_list ion-label button ion-icon{
		font-size:26px;
	}
	.user_list ion-label h2{
	    font-weight: 600;
	    color: #313135;
	}
	.user_list ion-label p{
	    color: #878686;
	}
	.user_list ion-label p span{
		margin-right:10px;
	}
	.user_list ion-label p span ion-icon{
		margin-right:5px;
		color:#002b3d;
	}
	.comment{
		border-bottom: 1px solid #d2d2d2;
	}
	.comment .label{
		margin:0;
	}
	.input-comment{
		position:relative;
	}
	.input-comment input{
		border:none;
		width:100%;
		height: 45px;
		padding-right: 45px;
	}
	.input-comment button{
		position:absolute;
	    right: 10px;
	    top: 15%;
		color:#002b3d;
	}
	.vote_images{
		width:100%;
	    height: 200px;
	    object-fit: cover;
	}
	.play_content{
		padding: 15px;
		display:flex;
		align-items:center;
		justify-content: space-between;
		margin-top:-4px;
		background-color: #f1f4f5;
	}
	.play_content h2{
	    font-weight: 600;
	    color: #313135;
	    margin:0 0 2px;
	}
	.play_content p{
	    color: #878686;
	    margin:0;
	}
	.play_content p span{
		margin-right:10px;
	}
	.play_content p span ion-icon{
		margin-right:5px;
		color:#002b3d;
	}
	.play_content span button{
		color:#002b3d;
		font-size: 14px;
		width:2em !important;
	}
}
