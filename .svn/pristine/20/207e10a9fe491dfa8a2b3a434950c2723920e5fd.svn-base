page-home {
	.images{
		width: 100%;
	    border-radius:50px;
	    height:100%;
	}
	.images_header{
		width: 20%;
		display: block;
	    margin-left: auto;
	    margin-right: auto;
	}
	.bg{
		background-color:#0c2a41;
	}
	.icon_header{
		color: white;
        font-size: 25px;
	}
	.btn_tab{
		color:#0c2a41;
		font-style:bold;
		border-radius:0px;
	}
	.btn_tab:focus, .btn_tab:hover, .btn_tab:active,.btn_tab.activated
	{
	  color: #0c2a41;
	  border-bottom: 2px solid #dd892b !important;
	  border-radius:0px;
	}
	.ionc_color{
		color:#0c2a41;
	}
	.sub_name{
		color:grey;
	}
	.name{
		font-weight:bold;
		color:black;
	}
	.tabs{
	    display: block;
	    margin-left: auto;
	    margin-right: auto;
	    text-align: center;
	}	
	.button-md{
		width:30%;
		border:none!important;
		background-color:white;
		color:black;
	}
	

}
    