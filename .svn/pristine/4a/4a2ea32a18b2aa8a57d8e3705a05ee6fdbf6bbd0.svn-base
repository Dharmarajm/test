page-login {
	

   .ion-content {
	    background: url("../assets/imgs/bg.jpg") no-repeat center center fixed; 	  
     }
    .images_logo{
    	text-align:center;
    	padding: 40px 60px 0px;
    }
	.item-input-has-focus .label-md[floating], .input-has-focus .label-md[floating] {
	    color: #fdc629;
	}
	.item-md.item-input.item-input-has-focus .item-inner, .item-md.item-input.input-has-focus .item-inner {
	    border-bottom-color: #fdc629;
	    -webkit-box-shadow: inset 0 -1px 0 0 #fdc629;
	    box-shadow: inset 0 -1px 0 0 #fdc629;
	}
	.item-md.item-input {
	    padding-left: 0px;
	    color: #fff;
	    background-color: transparent;
	    font-size: 16px;
	    font-weight: 300;
	}
	.login-form {
	    padding: 0px 35px;
	}
	.item-checkbox .item-inner {
	    border-bottom: none !important;
	}
	.checkbox-md .checkbox-icon {
	    border-radius: 1px;
	    border-color: #f4f4f4;
	    background-color: transparent;
	}
	.item.item-md.item-checkbox{
	    font-size: 14px;
	    font-weight: 300;
	    color: #c7c9cd;
	    padding: 10px 0;
	    background-color: transparent;
	}
	.item.item-md .checkbox-md {
	    position: static;
	    display: block;
	    margin: 9px 10px 9px 4px;
	}
	.checkbox-md .checkbox-checked .checkbox-inner {
	    border-color: #fdc629;
	}
}

 