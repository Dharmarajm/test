page-history {
	.subheader h3 span{
		font-size: 1.3rem;
		font-weight:900;
		color:#002b3d;
		padding:10px;
		background-color:#f1f4f5;
		margin-left:5px;
		border-radius:1px;
	}
	.button-group {
	    padding: 10px;
	    text-align: center;
	    margin-bottom: 5px;
	}
	.button-group button {
		min-width:75px;
	    border-radius: 6px;
	    height: 35px;
	    font-size: 1.3rem;
	    text-transform: uppercase;
	    font-weight: 600;
	}
	.button-group button:first-child ion-icon{
		margin:0 8px 0 0;
		font-size:14px;
	}
	.button-group button:last-child ion-icon{
		margin:0 0 0 8px;
		font-size:14px;
	}
	.button-group button.activated{
		background-color: #fccb2b;
		color:#002b3d;
	}
	.no_data{
		text-align: center;
	}

}
    