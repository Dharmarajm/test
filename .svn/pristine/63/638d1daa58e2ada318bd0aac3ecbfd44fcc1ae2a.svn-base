<ion-header class="bg"> 
  <ion-grid>
    <ion-row>
      <ion-col col-1>
        <ion-icon menuToggle class="ti ti-menu icon_header" ></ion-icon>
      </ion-col>
      <ion-col col-9>
        <img src="assets/imgs/logo.png" class="images_header">
      </ion-col>
      <ion-col col-1>
        <i class="ti ti-bell icon_header"></i>
      </ion-col>
      <ion-col col-1>
        <i class="ti ti-power-off icon_header" (click)="logout()" ></i>
      </ion-col>
    </ion-row>
  </ion-grid>
</ion-header>
<ion-content class="ion-content-profile">
    <br><br><br>
    <h5>CHANGE PASSWORD</h5>
    <div class="group">
          <input class="inputMaterial lower" id="email" type="text"  ng-model="user.email" required>
          <span class="highlight"></span>
          <div class="label">Email-ID</div>
   </div>   
    <div class="group">
          <input class="inputMaterial" id="password" type="text"  ng-model="user.password" required>
          <span class="highlight"></span>
          <div class="label">New Password</div>
    </div>
    <div class="group">
          <input class="inputMaterial" id="retry" type="text"  ng-model="user.retry" required>
          <span class="highlight"></span>
          <div class="label">Re-try</div>
    </div>
    <div class="group1">       
          <button ion-button block class="button-blue-profile"> NEXT</button>
    </div>
</ion-content>
